  NoteHeader: "\u041F\u0438\u0441\u044C\u043C\u043E \u043E\u0442 \u043E\u0442\u0446\u0430"
  NoteText: "\u041F\u0440\u0438\u0432\u0435\u0442, \u0441\u044B\u043D! \u041E\u0447\u0435\u043D\u044C
    \u0436\u0430\u043B\u044C, \u0447\u0442\u043E \u044F \u043D\u0435 \u0441\u043C\u043E\u0433
    \u0432\u0441\u0442\u0440\u0435\u0442\u0438\u0442\u044C \u0442\u0435\u0431\u044F
    \u043B\u0438\u0447\u043D\u043E. \u041A \u0441\u043E\u0436\u0430\u043B\u0435\u043D\u0438\u044E,
    \u044F \u043F\u043E\u043F\u0430\u043B \u0432 \u043D\u0435\u043F\u0440\u0438\u044F\u0442\u043D\u0443\u044E
    \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044E. \u042F \u0434\u043E\u043B\u0436\u0435\u043D
    \u0431\u044B\u043B \u0434\u043E\u0441\u0442\u0430\u0432\u0438\u0442\u044C \u043E\u0447\u0435\u043D\u044C
    \u0432\u0430\u0436\u043D\u0443\u044E \u043F\u043E\u0441\u044B\u043B\u043A\u0443,
    \u043D\u043E \u043F\u043E \u043F\u0443\u0442\u0438 \u043C\u0435\u043D\u044F \u043E\u0433\u0440\u0430\u0431\u0438\u043B\u0438
    \u0431\u0430\u043D\u0434\u0438\u0442\u044B.\n\n\u042F \u0437\u043D\u0430\u044E,
    \u0433\u0434\u0435 \u0443 \u043D\u0438\u0445 \u043B\u0430\u0433\u0435\u0440\u044C,
    \u043F\u043E\u044D\u0442\u043E\u043C\u0443 \u0441\u0435\u0439\u0447\u0430\u0441
    \u044F \u043E\u0442\u043F\u0440\u0430\u0432\u043B\u044F\u044E\u0441\u044C \u0442\u0443\u0434\u0430.
    \u041F\u043E\u0441\u0442\u0430\u0440\u0430\u044E\u0441\u044C \u0434\u043E\u0433\u043E\u0432\u043E\u0440\u0438\u0442\u044C\u0441\u044F
    \u0438\u043B\u0438 \u0432\u044B\u043A\u0440\u0430\u0441\u0442\u044C \u044D\u0442\u0443
    \u043F\u043E\u0441\u044B\u043B\u043A\u0443.\n\u041F\u043E\u043D\u0438\u043C\u0430\u044E,
    \u0447\u0442\u043E \u044D\u0442\u043E \u0440\u0438\u0441\u043A\u043E\u0432\u0430\u043D\u043D\u043E,
    \u043D\u043E \u044F \u043E\u0431\u044F\u0437\u0430\u043D \u0432\u044B\u043F\u043E\u043B\u043D\u0438\u0442\u044C
    \u044D\u0442\u043E\u0442 \u0437\u0430\u043A\u0430\u0437! \u0415\u0441\u043B\u0438
    \u044F \u044D\u0442\u043E\u0433\u043E \u043D\u0435 \u0441\u0434\u0435\u043B\u0430\u044E,
    \u043F\u043E\u0441\u043B\u0435\u0434\u0441\u0442\u0432\u0438\u044F \u043C\u043E\u0433\u0443\u0442
    \u0431\u044B\u0442\u044C \u043A\u0430\u0442\u0430\u0441\u0442\u0440\u043E\u0444\u0438\u0447\u0435\u0441\u043A\u0438\u043C\u0438
    \u0434\u043B\u044F \u0432\u0441\u0435\u0433\u043E \u043A\u043E\u0440\u043E\u043B\u0435\u0432\u0441\u0442\u0432\u0430.\n\n\u0421\u044B\u043D,
    \u044F \u043E\u0441\u0442\u0430\u0432\u043B\u044F\u044E \u0434\u043B\u044F \u0442\u0435\u0431\u044F
    \u0441\u0432\u043E\u044E \u043F\u043E\u0432\u043E\u0437\u043A\u0443 \u0438 \u0441\u0432\u043E\u0435\u0433\u043E
    \u043E\u0441\u043B\u0430. \u0412 \u043F\u043E\u0441\u043B\u0435\u0434\u043D\u0435\u0435
    \u0432\u0440\u0435\u043C\u044F \u0443 \u043C\u0435\u043D\u044F \u0442\u0443\u0433\u043E
    \u0441 \u0434\u0435\u043D\u044C\u0433\u0430\u043C\u0438, \u0434\u0430 \u0438
    \u043F\u043E\u0432\u043E\u0437\u043A\u0430 \u043D\u0443\u0436\u0434\u0430\u0435\u0442\u0441\u044F
    \u0432 \u0440\u0435\u043C\u043E\u043D\u0442\u0435 \u043F\u043E\u0441\u043B\u0435
    \u0430\u0442\u0430\u043A\u0438 \u0431\u0430\u043D\u0434\u0438\u0442\u043E\u0432.\n\u0412
    \u043F\u043E\u0432\u043E\u0437\u043A\u0435 \u0442\u044B \u043D\u0430\u0439\u0434\u0435\u0448\u044C
    \u043A\u043E\u0435-\u043A\u0430\u043A\u0438\u0435 \u0442\u043E\u0432\u0430\u0440\u044B,
    \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u0441\u043C\u043E\u0436\u0435\u0448\u044C
    \u043F\u0440\u043E\u0434\u0430\u0442\u044C.\n\n\u042F \u043F\u0440\u043E\u0448\u0443
    \u0442\u0435\u0431\u044F \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u0442\u044C\u0441\u044F
    \u0432 \u0434\u0435\u0440\u0435\u0432\u043D\u044E \u0421\u043E\u0441\u043D\u043E\u0432\u043A\u0430
    (\u044D\u0442\u043E \u043D\u0430 \u0441\u0435\u0432\u0435\u0440\u043E-\u0432\u043E\u0441\u0442\u043E\u043A
    \u043E\u0442 \u0434\u0435\u0440\u0435\u0432\u043D\u0438, \u0433\u0434\u0435 \u0436\u0438\u0432\u0435\u0442
    \u0422\u043E\u043C\u0430\u0441). \u041D\u0430\u0439\u0434\u0438 \u0442\u0430\u043C
    \u0441\u0442\u0430\u0440\u0438\u043A\u0430 \u0420\u0438\u0447\u0430\u0440\u0434\u0430,
    \u044D\u0442\u043E \u043C\u043E\u0439 \u0437\u043D\u0430\u043A\u043E\u043C\u044B\u0439
    \u0442\u043E\u0440\u0433\u043E\u0432\u0435\u0446, \u0445\u043E\u0442\u044C \u0438
    \u043D\u0435\u043F\u0440\u0438\u044F\u0442\u043D\u044B\u0439 \u0442\u0438\u043F.
    \u0415\u0441\u043B\u0438 \u0432\u0441\u0451 \u043F\u0440\u043E\u0439\u0434\u0435\u0442
    \u0445\u043E\u0440\u043E\u0448\u043E, \u044F \u0431\u0443\u0434\u0443 \u0436\u0434\u0430\u0442\u044C
    \u0442\u0435\u0431\u044F \u0443 \u043D\u0435\u0433\u043E.\n\n\u042F \u0435\u0449\u0451
    \u0440\u0430\u0437 \u043F\u0440\u043E\u0448\u0443 \u0443 \u0442\u0435\u0431\u044F
    \u043F\u0440\u043E\u0449\u0435\u043D\u0438\u044F \u0437\u0430 \u0432\u0441\u0451,
    \u043D\u043E \u0443 \u043C\u0435\u043D\u044F \u043D\u0435\u0442 \u0434\u0440\u0443\u0433\u043E\u0433\u043E
    \u0432\u044B\u0445\u043E\u0434\u0430. \u0421\u044B\u043D, \u0437\u043D\u0430\u0439,
    \u0447\u0442\u043E \u044F \u043B\u044E\u0431\u043B\u044E \u0442\u0435\u0431\u044F.\n\t\t\t-\u041E\u0442\u0435\u0446"
